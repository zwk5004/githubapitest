            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="vler:PersonIdentifier" minOccurs="0"/>
                    <xsd:element ref="vler:ServiceProviderRoleTitle"/>
                    <xsd:element ref="nc:ContactInformation" minOccurs="0"/>
                    <xsd:element ref="nc:RoleOfPersonReference"/>
                    <xsd:element ref="nc:OrganizationReference" minOccurs="0"/>
                    <xsd:element ref="nc:FacilityReference" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="ExamDetail">
        <xsd:annotation>
            <xsd:documentation>Holds details about the exam being sent</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="vler:ExamDetailType">
                    <xsd:sequence>
                        <xsd:element ref="vler:PhysicalSource"/>
                        <xsd:element ref="vler:ExamId">
                            <xsd:annotation>
                                <xsd:documentation>Unique identifier assigned to a request for a C &amp; P Exam. CAPRI
                                    assigns the Exam Request ID for VHA C &amp; P exams. CCR assigns the Exam ID for
                                    contracted vendor C &amp; P exams.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element ref="vler:ExaminerType"/>
                        <xsd:element ref="vler:ContractType"/>
                        <xsd:element ref="vler:StatusDate"/>
                        <xsd:element ref="vler:Comments"/>
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="ExamDetailType">
        <xsd:annotation>
            <xsd:documentation>A data type for the Exam Detail</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="PhysicalSource" type="vler:PhysicalSourceType">
        <xsd:annotation>
            <xsd:documentation>The physical source of the exam result. The entity that generated the exam result. (i.e.,
                Veterans Evaluation Service=‘VES', CAPRI=‘VHA_CAPRI’, , Quality, Timeliness, Customer Service Management
                Inc.='QTC', Veteran Federal Resources Inc.=‘VetFed’, Medical Services of Los Angeles=‘MSLA’, Logistics
                Health Incorporated=‘LHI’ etc.)
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="PhysicalSourceType">
        <xsd:annotation>
            <xsd:documentation>A data type for the PhysicalSource</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="nc:TextType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="ExamId" type="vler:ExamIdType">
        <xsd:annotation>
            <xsd:documentation>Unique identifier assigned to a request for a C &amp; P Exam. CAPRI assigns the Exam ID
                for VHA C &amp; P exams. CCR assigns the Exam ID for contracted vendor C &amp; P exams.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ExamIdType">
        <xsd:annotation>
            <xsd:documentation>A data type for the ExamIdType</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="nc:TextType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="ExaminerType" type="vler:ExaminerTypeType">
        <xsd:annotation>
            <xsd:documentation>The relationship of the examiner to the VA, whether internal to VA = “Inhouse”,
                contracted by the VA = “Contracted”, or unknown/external to VA = “External”.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ExaminerTypeType">
        <xsd:annotation>
            <xsd:documentation>A data type for the ExaminerType</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="nc:TextType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="ContractType" type="vler:ContractTypeType">
        <xsd:annotation>
            <xsd:documentation>The VA administration through which the contract was secured (i.e., ‘VHA’ or ‘VBA’).
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ContractTypeType">
        <xsd:annotation>
            <xsd:documentation>A data type for the ContractType</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="nc:TextType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="StatusDate" type="vler:StatusDateType">
        <xsd:annotation>
            <xsd:documentation>The date that the status of the exam result was initially assigned or changed. For
                example, the date that the VHA approver set the exam status to ‘Rejected’.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="StatusDateType">
        <xsd:annotation>
            <xsd:documentation>A data type for the StatusDate</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="nc:DateType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="Comments" type="vler:CommentsType">
        <xsd:annotation>
            <xsd:documentation>Free text field where comments from the exam requester, examiner, or approver are
                recorded and conveyed between entities.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="CommentsType">
        <xsd:annotation>
            <xsd:documentation>A data type for the Comments</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="nc:TextType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--</version 1.4>-->
</xsd:schema>
