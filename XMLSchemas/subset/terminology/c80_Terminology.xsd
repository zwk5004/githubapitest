            <appinfo>
                <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
                <v1:CodingValueSet codeSystem="2.16.840.1.113883.6.96" codeSystemName="SNOMED CT"
                                   valueSet="2.16.840.1.113883.3.88.12.3221.7.2"/>
            </appinfo>
        </annotation>
        <restriction base="c80_term:SNOMEDCodeSimpleType">
            <enumeration value="404684003">
                <annotation>
                    <documentation>Finding</documentation>
                </annotation>
            </enumeration>
            <enumeration value="418799008">
                <annotation>
                    <documentation>Symptom</documentation>
                </annotation>
            </enumeration>
            <enumeration value="55607006">
